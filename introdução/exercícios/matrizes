package com.mycompany.testesjava;
public class Testesjava {

    public static void main(String[] args) {
        
            String[][] foods = { 
                {"Hamburguer", "Pizza", "Sushi"},
                {"Cake", "Ice cream", "Cookie"},
                {"Tacos", "Burritos", "Quesadillas"}
         };
            
            for (int i = 0; i < foods.length; i++) {
                System.out.println();   // Adiciona uma nova linha antes de cada linha da matriz
                for (int j = 0; j < foods[i].length; j++) {
                    System.out.print(foods[i][j] + " "); //Imprime os elementos na mesma linha

                }
            }
     }
}
